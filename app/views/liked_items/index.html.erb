<h1>Listing liked_items</h1>

<table>
  <tr>
    <th>User</th>
    <th>Product</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @liked_items.each do |liked_item| %>
  <tr>
    <td><%= liked_item.user_id %></td>
    <td><%= liked_item.product_id %></td>
    <td><%= link_to 'Show', liked_item %></td>
    <td><%= link_to 'Edit', edit_liked_item_path(liked_item) %></td>
    <td><%= link_to 'Destroy', liked_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Liked item', new_liked_item_path %>
